{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/applicationNew.json",
    "type": "object",
    "properties": {
      "userId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "grantId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "grant_response": {
        "type": "string",
        "minLength": 1,
        "maxLength": 500
      },
      "application_status": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "application_submission_date": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "application_response_date": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      }
    },
    "additionalProperties": false,
    "required": ["userId", "grantId", "grant_response"]
  }
  