{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/programNew.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100
        },
        "link": {
            "type": "string",
            "format": "uri"
        },
        "description": {
            "type": "string",
            "minLength": 1
        }
    },
    "additionalProperties": false,
    "required": ["title", "link", "description"]
}
