{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/userUpdate.json",
    "type": "object",
    "properties": {
      "first_name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "last_name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "password": {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      },
      "email": {
        "type": "string",
        "minLength": 6,
        "maxLength": 60,
        "format": "email"
      }
    },
    "additionalProperties": true
  }
  