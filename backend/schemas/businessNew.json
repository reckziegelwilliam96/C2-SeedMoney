{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/businessNew.json",
    "type": "object",
    "properties": {
        "business_name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100
        },
        "business_address": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100
        },
        "tax_id": {
            "type": "string",
            "minLength": 9,
            "maxLength": 9
        },
        "user_id": {
            "type": "integer",
            "minimum": 1
        }
    },
    "additionalProperties": false,
    "required": ["business_name", "business_address", "tax_id", "user_id"]
}
