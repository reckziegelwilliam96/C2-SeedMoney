{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/applicationNew.json",
  "type": "object",
  "properties": {
      "user_id": {
          "type": "number"
      },
      "grant_id": {
          "type": "number"
      },
      "farm_name": {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
      },
      "farm_location": {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
      },
      "farm_size": {
          "type": "number"
      },
      "farm_revenue": {
          "type": "number"
      },
      "crops_grown": {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
      },
      "animals_raised": {
          "type": "string",
          "minLength": 1,
          "maxLength": 255
      },
      "app_proposal": {
          "type": "string",
          "minLength": 1,
          "maxLength": 500
      }
  },
  "additionalProperties": false,
  "required": ["user_id", "grant_id", "farm_name", "farm_location", "farm_size", "farm_revenue", "crops_grown", "animals_raised", "app_proposal"]
}
